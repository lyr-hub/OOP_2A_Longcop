class Phone:
    def __init__(self, brand,model):
        self.brand = brand
        self.model = model
    def describePhone(self):
        print(f"The Phone's brand {self.brand} the model of it is {self.model}.")
       
class Andriod(Phone):
    def __init__(self, brand,model):
        Phone.__init__(self,brand,model)

iphone = Andriod("iphone", "xr")
iphone.describePhone()


